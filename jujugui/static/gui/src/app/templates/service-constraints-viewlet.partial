{{#constraints}}
  <div class="settings-wrapper">
   <label for="{{name}}">{{title}}</label>
    <div>
      <input class="constraint-field {{name}}" type="text" name="{{name}}"
          value="{{value}}" data-bind="constraints.{{name}}" />
      {{#if unit }}<span class="text-in-input">{{unit}}</span>{{/if}}
    </div>
    {{>conflict-ux}}
  </div>
{{/constraints}}
