<div class="viewlet-manager-wrapper">
  <div class="header-slot"></div>
  <ul class="viewlet-manager-navigation">
    <li>
        <a href="" class="tab active" data-viewlet="overview">
            <i class="sprite show_hover_16 normal"></i>
            <i class="sprite show_hover_16 selected"></i>
        </a>
    </li>
    <!-- Settings control -->
    <li>
        <a href="" class="tab" data-viewlet="config">
            <i class="sprite settings_16 normal"></i>
            <i class="sprite settings_16 selected"></i>
        </a>
    </li>
    <!-- Relations control -->
    <li>
        <a href="" class="tab" data-viewlet="relations">
            <i class="sprite relation_16 normal"></i>
            <i class="sprite relation_16 selected"></i>
        </a>
    </li>
  </ul>
  <div class="viewlet-container"></div>
  <div class="viewlet-manager-footer">
    <div class="overview-footer-item">
      {{>service-version}}
      {{>service-destroy}}
    </div>
  </div>
</div>
