    "charm-metadata": {
      "Name": "puppetmaster",
      "Summary": "puppet master",
      "Description": "This charm deploys a puppet master, which is the server hosting\nmanifests and files for the puppet nodes.  Puppet lets you centrally\nmanage every important aspect of your system using a cross-platform\nspecification language that manages all the separate elements normally\naggregated in different files, like users, cron jobs, and hosts, along\nwith obviously discrete elements like packages, services, and files.\nPuppet's simple declarative specification language provides powerful\nclassing abilities for drawing out the similarities between hosts while\nallowing them to be as specific as necessary, and it handles dependency\nand prerequisite relationships between objects clearly and explicitly.\n",
      "Subordinate": false,
      "Provides": {
        "puppetmaster": {
          "Name": "puppetmaster",
          "Role": "provider",
          "Interface": "puppet",
          "Optional": false,
          "Limit": 0,
          "Scope": "global"
        }
      },
