{
  "name": "Juju GUI",
  "manifest_version": 2,
  "version": "2.2.2",

  "description": "Manage your Juju environment",
  "icons": {
    "128": "app/assets/images/juju-128.png"
  },

  "app": {
    "urls": [
      "*://localhost/"
    ],
    "launch": {
      "web_url": "http://localhost:8888/"
    }
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
